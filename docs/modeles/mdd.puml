@startuml MDD
skinparam style strictuml
skinparam ArrowThickness 2
skinparam ArrowColor #gray

hide empty members
title Modèle du domaine

together {

  class Utilisateur {
    codePermanent : String
    nom : String
    prenom : String
    courriel : Sring
    motDePasse : String
  }
  class Étudiant {

  }
  class Enseignant {
    bureau : String
  }

}

together {

  class CatalogueGroupeCours {}

  class Cours {
    sigle : String
    nbCreduits : Number
  }

  class GroupeCours {
    numGroupe : Number
    nbParticipant: int
  }
  
  class Session {
    nomAbrege : String 
    nomComplet : String
    trimestre : Number 
    annee : Number
  }
  together {

  }
}


together {
  class Question {
    idQuestion : Number
    nom : String
    énoncé : String
    msgRétroactionVrai : String
    msgRétroactionFaux : String
  }
  class TypeQuestionGIFT {}

  class Questionnaire {
    idQuestionnaire : Number
    nom : String
    description : String
  }
}

' associations
Utilisateur  "l" <|-- Étudiant
Utilisateur "l" <|-- Enseignant 

CatalogueGroupeCours "1" -- "*" GroupeCours :  Répertorie >
Cours "1" -- "1" GroupeCours : Décrit >
Enseignant "1" -- "*" GroupeCours : Donne > 
GroupeCours "1" -- "*" Questionnaire : Exige > 
GroupeCours "1-*" -- "1" Session : Est-donnée > 
Questionnaire "1" -- "*" Question : Contient >
Question "1" -- "1" TypeQuestionGIFT : Est-de-type >
@enduml